    "import sys\n",
    "\n",
    "N=int(input())\n",
    "A=[list(map(int,input().split())) for _ in range(N)]\n",
    "\n",
    "start=N//2\n",
    "sum_apple=sum(A[start])\n",
    "for i in range(N//2):\n",
    "    sum_apple+=sum(A[i][start-i:start+i+1])\n",
    "    sum_apple+=sum(A[-i-1][start-i:start+i+1])\n",
    "print(sum_apple)"
   ]
