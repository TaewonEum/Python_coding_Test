    "N=int(input())\n",
    "\n",
    "for i in range(N):\n",
    "    New=''\n",
    "    A=input()\n",
    "    A=A.upper()\n",
    "    for j in reversed(A):\n",
    "        New+=j\n",
    "    if A==New:\n",
    "        print(f'#{i+1} YES')\n",
    "    else:\n",
    "        print(f'#{i+1} NO')"
