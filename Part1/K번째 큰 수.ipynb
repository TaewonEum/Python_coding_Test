   "source": [
    "import sys\n",
    "N,K=map(int,input().split())\n",
    "a=list(map(int,input().split()))\n",
    "res=set()\n",
    "for i in range(N):\n",
    "    for j in range(i+1,N):\n",
    "        for k in range(j+1,N):\n",
    "            res.add(a[i]+a[j]+a[k])\n",
    "res=list(res)\n",
    "res.sort(reverse=True)\n",
    "print(res[K-1])"
   ]
