    "N=int(input())\n",
    "A=list(map(int,input().split()))\n",
    "M=int(input())\n",
    "\n",
    "for _ in range(M):\n",
    "    maxx=max(A)\n",
    "    minn=min(A)\n",
    "    idx=A.index(maxx)\n",
    "    idx_1=A.index(minn)\n",
    "    A[idx]=A[idx]-1\n",
    "    A[idx_1]=A[idx_1]+1\n",
    "    \n",
    "print(max(A)-min(A))"
   ]
