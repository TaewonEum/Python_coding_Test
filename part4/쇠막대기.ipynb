   "source": [
    "s=input()\n",
    "stack=[]\n",
    "cnt=0\n",
    "for i in range(len(s)):\n",
    "    if s[i]=='(':\n",
    "        stack.append(s[i])\n",
    "    else:\n",
    "        stack.pop()\n",
    "        if s[i-1]=='(':\n",
    "            cnt+=len(stack)\n",
    "        else:\n",
    "            cnt+=1\n",
    "\n",
    "print(cnt)"
   ]
